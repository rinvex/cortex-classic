{
    "name": "rinvex/cortex",
    "description": "Rinvex Cortex is a solid foundation for enterprise solutions, that provides a flexible and extensible architecture for building multi-lingual, multi-tenant applications with content management, themeable views, application modules and much more.",
    "keywords": [
        "rinvex",
        "cortex",
        "flexible",
        "framework",
        "laravel",
        "themes",
        "solution",
        "extension",
        "enterprise",
        "application",
        "multi-lingual",
        "multi-tenant",
        "bootstrap",
        "module"
    ],
    "type": "project",
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/country/issues",
        "source": "https://github.com/rinvex/country",
        "docs": "https://github.com/rinvex/country/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/country/contributors"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "aws/aws-sdk-php": "~3.0",
        "aws/aws-sdk-php-laravel": "~3.0",
        "barryvdh/laravel-snappy": "dev-master as 0.3.3",
        "cortex/attributable": "dev-develop",
        "cortex/categorizable": "dev-develop",
        "cortex/fort": "dev-develop",
        "cortex/foundation": "dev-develop",
        "cortex/taggable": "dev-develop",
        "cortex/tenantable": "dev-develop",
        "doctrine/dbal": "^2.6",
        "dusterio/laravel-aws-worker": "~0.1",
        "fideloper/proxy": "^3.3",
        "kalnoy/nestedset": "dev-master as 4.2.7",
        "laravel/envoy": "~1.0",
        "laravel/framework": "~5.5.0",
        "laravel/tinker": "^1.0.0",
        "laravelcollective/html": "dev-master as 5.4.8",
        "lord/laroute": "^2.4.0",
        "maatwebsite/excel": "dev-master as 2.1.17",
        "predis/predis": "^1.1.0",
        "rollbar/rollbar-laravel": "^2.0",
        "spatie/eloquent-sortable": "dev-master as 3.3.0",
        "spatie/laravel-activitylog": "dev-master as 1.15",
        "spatie/laravel-fractal": "dev-master as 4.0.0",
        "spatie/laravel-sluggable": "dev-master as 1.5.0",
        "spatie/laravel-translatable": "dev-master as 1.3.0",
        "yajra/laravel-datatables-oracle": "dev-master as 7.8.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4.0",
        "barryvdh/laravel-debugbar": "^2.4.0",
        "itsgoingd/clockwork": "^1.14",
        "phpunit/phpunit": "^6.0.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/rinvex/l55-laravel-activitylog"
        },
        {
            "type": "vcs",
            "url": "https://github.com/rinvex/l55-laravel-fractal"
        },
        {
            "type": "vcs",
            "url": "https://github.com/rinvex/l55-laravel-translatable"
        },
        {
            "type": "vcs",
            "url": "https://github.com/rinvex/l55-laravel-sluggable"
        },
        {
            "type": "vcs",
            "url": "https://github.com/rinvex/l55-eloquent-sortable"
        },
        {
            "type": "vcs",
            "url": "https://github.com/rinvex/l55-laravel-nestedset"
        },
        {
            "type": "vcs",
            "url": "https://github.com/rinvex/l55-laravel-snappy"
        },
        {
            "type": "vcs",
            "url": "https://github.com/rinvex/l55-html"
        },
        {
            "type": "vcs",
            "url": "https://github.com/rinvex/l55-laravel-datatables"
        },
        {
            "type": "vcs",
            "url": "https://github.com/rinvex/l55-Laravel-Excel"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan ide-helper:generate",
            "@php artisan laroute:generate",
            "@php artisan package:discover",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
